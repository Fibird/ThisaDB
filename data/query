create index ORDERS(O_ORDERKEY);
create index ORDERS(O_ORDERPRIORITY);
reset buffer;
reset io;

select O_ORDERKEY, O_ORDERSTATUS, O_ORDERDATE, O_ORDERPRIORITY, O_CLERK
from ORDERS
where O_ORDERKEY = 49600;

select O_ORDERKEY, O_ORDERSTATUS, O_ORDERDATE, O_ORDERPRIORITY, O_CLERK
from ORDERS
where O_ORDERKEY = 9988;

select O_ORDERKEY, O_ORDERSTATUS, O_ORDERDATE, O_ORDERPRIORITY, O_CLERK
from ORDERS
where O_ORDERKEY = 59074;

select O_ORDERKEY, O_ORDERSTATUS, O_ORDERDATE, O_ORDERPRIORITY, O_CLERK
from ORDERS
where O_ORDERPRIORITY = "5-LOW";

print io;



create index CUSTOMER(C_PHONE);
reset buffer;
reset io;

select C_NAME, C_ADDRESS, C_PHONE
from CUSTOMER
where C_PHONE > "34-6";

select C_NAME, C_ADDRESS, C_PHONE
from CUSTOMER
where C_PHONE > "11-8";

print io;

exit;
create index LINEITEM(L_ORDERKEY);
reset buffer;
reset io;

select C_NAME, C_ADDRESS, C_PHONE, C_MKTSEGMENT, O_ORDERDATE, O_ORDERPRIORITY, O_CLERK, O_COMMENT
from ORDERS, CUSTOMER
where C_CUSTKEY = O_CUSTKEY;

select O_CLERK, O_ORDERDATE, O_ORDERPRIORITY, L_SHIPDATE, L_SHIPMODE, O_COMMENT
from ORDERS, LINEITEM
where O_ORDERPRIORITY = "1-URGENT" and O_TOTALPRICE > 300000.0 and O_ORDERKEY = L_ORDERKEY;

print io;



reset buffer;
reset io;

select C_NAME, N_NAME, R_NAME
from CUSTOMER, NATION, REGION
where C_NATIONKEY = N_NATIONKEY and N_REGIONKEY = R_REGIONKEY;

print io;
